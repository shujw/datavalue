					<key>source</key>
					<string>on run {input, parameters}	tell application "Terminal"				if it is running then			activate			tell application "System Events"				keystroke "n" using command down #相当于  command+T  			end tell			delay 0.5			set Terminal2 to front window						#set tab2 to tab 1 of Terminal2			do script "cd /Users/kenny/Documents/02.we-tech/wechat/wxYuHanStore-master" in Terminal2			do script "npm run dev" in Terminal2			#do script "ls" in tab2		else						activate						set Terminal to front window			set tab1 to tab 1 of Terminal						# 窗口1中输出			do script "cd /Users/kenny/Documents/02.we-tech/wechat/wxYuHanStore-master" in tab1			do script "npm run dev" in tab1						# 或者ssh登录			# do script "ssh ***" in tab1			# delay 1    #延迟 1 秒			# do script "..." 			end if			end tell	return inputend run</string>
